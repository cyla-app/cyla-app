{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:apk": "cd android && ./gradlew assembleRelease && adb install app/build/outputs/apk/release/app-release.apk",
    "android:aab": "cd android && ./gradlew bundleRelease",
    "android:adb:reverse": "adb reverse tcp:5000 tcp:5000 && adb reverse tcp:8081 tcp:8081",
    "android:start-avd": "bash -c '$ANDROID_HOME/emulator/emulator -avd $($ANDROID_HOME/emulator/emulator -list-avds | head -n 1)'",
    "ios": "react-native run-ios",
    "start": "yarn generate && react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "generate:clean": "rimraf ./generated/*.ts",
    "postinstall": "patch-package",
    "generate:protobuf": "cd ../specs && protoc --plugin=../frontend/node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_opt=useOptionals=true --ts_proto_out=../frontend/generated/ ./period-stats.proto ./day.proto"
  },
  "dependencies": {
    "@formatjs/intl-datetimeformat": "^3.2.12",
    "@formatjs/intl-displaynames": "^4.0.10",
    "@formatjs/intl-getcanonicallocales": "^1.5.6",
    "@formatjs/intl-listformat": "^5.0.10",
    "@formatjs/intl-locale": "^2.4.19",
    "@formatjs/intl-numberformat": "^6.2.2",
    "@formatjs/intl-pluralrules": "^4.0.11",
    "@formatjs/intl-relativetimeformat": "^8.1.2",
    "@gorhom/bottom-sheet": "^3",
    "@react-native-async-storage/async-storage": "^1.13.4",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.10",
    "@react-native-community/slider": "^3.0.3",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/material-bottom-tabs": "^5.3.13",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.12.8",
    "@reduxjs/toolkit": "^1.5.0",
    "date-fns": "^2.16.1",
    "install": "^0.13.0",
    "npm": "^7.6.0",
    "protobufjs": "^6.10.2",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-calendar-strip": "^2.1.0",
    "react-native-calendars": "^1.505.0",
    "react-native-flipper": "^0.72.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-localize": "^2.0.2",
    "react-native-paper": "^4.7.1",
    "react-native-paper-dates": "^0.3.0",
    "react-native-reanimated": "2.10.0",
    "react-native-redash": "^16.0.4",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.18.1",
    "react-native-svg": "^12.1.0",
    "react-native-vector-icons": "^7.1.0",
    "react-redux": "^7.1.12",
    "redux": "^4.0.5",
    "redux-flipper": "^1.4.2",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-native-calendars": "^1.20.12",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-redux": "^7.1.12",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "mkdirp": "^1.0.4",
    "patch-package": "^6.2.2",
    "react-test-renderer": "16.13.1",
    "rimraf": "^3.0.2",
    "ts-proto": "^1.54.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
